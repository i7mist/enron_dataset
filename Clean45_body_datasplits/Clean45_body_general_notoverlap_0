Public Sub ReloadData( ) Dim ODBCstring As String Dim ws As Workspace , db As Database Dim rs As Recordset Dim curProd , oldProd As String Dim RowOffset As Integer Dim totalrow , tmpCount , i As Integer Dim today As String 'Dim next_date As String Dim userid As String Dim password As String ' ODBC connection string and database connection MsgBox "Fail to login to the database" Exit Sub End If ' get the total row that is used 'clear the old contents While Not rs.EOF With Worksheets("EOL" ) 'End If Else End If End With Wend With Worksheets("EOL" ) Else End If Next i End With End Sub
select REF_PERIOD_ENGLISH_SHORT_DESC,BUY_SELL_CD,reference_period_end_dtm,sum(transaction_qty ) from eol.TRADING_TRANSACTIONS WHERE TO_CHAR(transaction_dtm,'DD-MON-YYYY' ) = TO_CHAR(SYSDATE,'DD-MON-YYYY' ) and ref_period_english_short_desc like 'US Gas Swap%Nymex%' GROUP BY REF_PERIOD_ENGLISH_SHORT_DESC,BUY_SELL_CD,reference_period_end_dtm order by reference_period_end_dtm,BUY_SELL_CD
